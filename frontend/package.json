{
  "name": "frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run build:vite && npm run build:static && cp -rf ./dist/ ../target/classes/static/",
    "build:static": "npm run build:static:version && npm run build:static:api-docs",
    "build:static:api-docs": "node ./scripts/build-api.js",
    "build:static:version": "node ./scripts/build-version.js",
    "build:vite": "npx vite build",
    "lint": "npx eslint ./src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts",
    "lint:fix": "npx eslint ./src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "preview": "npx vite preview",
    "start:dev": "npm run build:static && echo '' && npx vite",
    "test:unit": "npx vitest",
    "type-check": "npx vue-tsc --build --force"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "sass": "^1.32.11",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.11"
  }
}
